@model IEnumerable<CapaModelo.tbSolictudVuelo>

@{
    ViewBag.Title = "Solicitud Charter - Especiales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>

<br />
<div class="card" style="font-size:12px">
    <div class="card-header bg-gradient-lightblue">
        <table style="width:100%">
            <tr>
                <td class="text-center">
                    <h6>DIRECCIÓN DE TRANSPORTE AÉREO</h6>
                </td>
            </tr>
            <tr>
                <td class="text-center">
                    <h6>GESTIÓN INTERNA DE TESORERÍA</h6>
                </td>
            </tr>
            <tr>
                <td class="text-center">
                    <h6>AUTORIZACIÓN DE LA SOLICITUD DE VUELOS CHARTER O ESPECIALES</h6>
                </td>
            </tr>

        </table>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <input type="hidden" id="pvista" name="pvista" value="@ViewBag.Vista" />
        <div class="text-justify">
            <label class="col-form-label"><a href="#"> Cargar información de fechas anteriores: <img src="~/Content/imganes/buscar.png" style="height:20px" data-toggle="modal" data-target="#modalbuscarFecha" /></a> </label>
        </div>
        <table id="tbDetalleCharter" class="table table-bordered table-hover table-sm" style="width:100%; font-size:x-small">
            <thead class="text-center">
                <tr>
                    <th>No. SOL.</th>
                    <th>FECHA SOLICITUD</th>
                    <th>FECHA APROBACIÓN</th>
                    <th>OPERACIÓN</th>
                    <th>ESTADO SOLICITUD</th>
                    <th>CIA. OPERADORA</th>
                    <th>CIA. FACTURA</th>
                    <th>No PERMISO</th>
                    <th>No VUELOS</th>
                    <th>RUTA VUELO</th>
                    <th>APROB. G.I. AUTORIZACIONES</th>
                    <th>APROB. FINANCIERO</th>
                    <th>APROB. TRANSP. AEREO</th>
                    <th>ACCIONES</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    string _numVuelos = item.NumeroVuelos.ToString();
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.NumeroSolicitud)</td>
                        <td class="text-center">@Html.DisplayFor(modelItem => item.FechaEnvioSolicitud)</td>
                        <td class="text-center">@Html.DisplayFor(modelItem => item.FechaTransporteAereo)</td>
                        <td class="text-justify">@Html.DisplayFor(modelItem => item.DescripcionTipoSolictud)</td>
                        <td class="text-center">@Html.DisplayFor(modelItem => item.EstadoSolicitud)</td>
                        <td class="text-justify">@Html.DisplayFor(modelItem => item.NombreCompaniaAviacion)</td>
                        <td class="text-justify">@Html.DisplayFor(modelItem => item.NombreFactura)</td>
                        <td>@Html.DisplayFor(modelItem => item.AutorizacionSolicitudVuelo)</td>
                        <td class="text-center">@_numVuelos</td>
                        <td>@Html.DisplayFor(modelItem => item.CboAeropuertoAterrizaje)</td>
                        <td>
                            @if (item.EstadoOperacionesDSOP.Equals("NE"))
                            {
                                <strong><img src='../Content/imganes/cancel.png' style='width:20px;' /> @item.DescripcionOperacionesDSOP </strong>
                            }
                            else if (item.EstadoOperacionesDSOP.Equals("AP"))
                            {
                                <strong><img src='../Content/imganes/check.png' style='width:20px;' /> @item.DescripcionOperacionesDSOP</strong>
                            }
                            else if (item.EstadoOperacionesDSOP.Equals("HC"))
                            {
                                <strong><img src='../Content/imganes/signo-menosAmarillo.png' style='width:20px;' /> HABILITAR CORRECCION</strong>
                            }
                            else if (item.EstadoOperacionesDSOP.Equals(""))
                            {
                                <strong><img src='../Content/imganes/button-minus.png' style='width:20px;' /> PENDIENTE </strong>
                            }


                        </td>
                        <td class="text-center">
                            @if (item.EstadoFinanciero.Equals(""))
                            {
                                if (item.EstadoOperacionesDSOP.Equals("AP") || item.EstadoOperacionesDSOP.Equals("NE"))
                                {
                                    <a href='#' onclick='#'><b style='color:#ff0000'><img src='../Content/imganes/menos.png' style='width:20px;' /> PENDIENTE</b></a>
                                }
                                else if (item.EstadoFinanciero.Equals(""))
                                {
                                    <strong><img src='../Content/imganes/button-minus.png' style='width:20px;' /> PENDIENTE</strong>
                                }
                            }
                            else if (item.EstadoFinanciero.Equals("HC"))
                            {
                                if (item.EstadoAutoriza.Equals("A"))
                                {
                                    <a href='#' onclick='abrirPopUpForm(@item.NumeroSolicitud)'><b style='color:#ff0000'><img src='../Content/imganes/signo-menosAmarillo.png' style='width:20px;' /> HABILITAR CORRECCION</b></a>
                                }
                                else
                                {
                                    <strong><img src='../Content/imganes/signo-menosAmarillo.png' style='width:20px;' /> HABILITAR CORRECCION </strong>
                                }
                            }
                            else if (item.EstadoFinanciero.Equals("AP"))
                            {
                                <strong><img src='../Content/imganes/check.png' style='width:20px;' /> @item.DescripcionEstadoFinanciero </strong>
                            }
                            else if (item.EstadoFinanciero.Equals("NE"))
                            {
                                <strong><img src='../Content/imganes/cancel.png' style='width:20px;' /> @item.DescripcionEstadoFinanciero </strong>
                            }

                        </td>
                        <td class="text-center">
                            @if (item.EstadoTransporteAereo.Equals(""))
                            {
                                <strong><img src='../Content/imganes/button-minus.png' style='width:20px;' /> PENDIENTE </strong>
                            }
                            else if (item.EstadoTransporteAereo.Equals("AP"))
                            {
                                <strong><img src='../Content/imganes/check.png' style='width:20px;' /> @item.DescripcionEstadoTransporteAereo </strong>
                            }
                            else if (item.EstadoTransporteAereo.Equals("NE"))
                            {
                                <strong><img src='../Content/imganes/cancel.png' style='width:20px;' /> @item.DescripcionEstadoTransporteAereo</strong>
                            }
                            else if (item.EstadoTransporteAereo.Equals("HC"))
                            {
                                <strong><img src='../Content/imganes/signo-menosAmarillo.png' style='width:20px;' /> HABILITAR CORRECCION</strong>
                            }

                        </td>
                        <td class="text-center">
                            <div class="btn-group">
                                @if (item.EstadoResponsableATO != "" && item.AutorizacionSolicitudVuelo.Trim().Length > 0)
                                {
                                    <a id='imprimir' class="btn btn-default btn-sm" href='#' onclick='imprimir("@item.AutorizacionSolicitudVuelo")'><img src='../Content/imganes/printer.png' style='width:20px' /></a>
                                }
                                <a href='#' class="btn btn-default btn-sm" onclick='abrirSolicitud("@item.NumeroSolicitud")'><img src='../Content/imganes/consultar-documento.png' style='width:20px' data-id="@item.NumeroSolicitud" data-toggle="tooltip" data-placement="left" title="Consultar solicitud" /></a>
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    @if (ViewBag.mensajeError != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                Swal.fire({
                    title: 'Autorización Vuelos Chárter',
                    html: set(@Html.Raw(Json.Encode(ViewBag.mensajeError))),
                    icon: 'warning',
                    showCancelButton: false,
                    confirmButtonColor: '#3085d6',
                    confirmButtonText: 'Aceptar',
                    allowOutsideClick: false,
                });
                                };
                                function set(value) {
                                    return value;
                                }
        </script>
    }
    <!-- /.card-body -->
</div>
<!-- /.card -->
<!-- Modal -->
<div class="modal fade" id="FormModal" tabindex="-1" role="dialog" data-backdrop="static" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header bg-gradient-primary text-white">
                <h6 class="modal-title text-white" id="modalLabel"></h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="font-size:12px">
                <div style="height:400px">
                    <div class="container-fluid h-100 d-flex flex-column">
                        <div class="row overflow-hidden">
                            <div class="col-sm-12 h-100">
                                <div class="card h-100">
                                    <div class="card-body overflow-auto">
                                        <input type="hidden" id="txtTipoSolicitud" />
                                        <table style="width:100%; border-collapse: separate; border-spacing: 5px; background-color:#f2f1d0">
                                            <tr>
                                                <td class="col-sm-4 text-sm align-top">
                                                    <small class="card-title text-muted"><u class="text-primary">TIPO DE SOLICITUD</u></small>
                                                </td>
                                                <td>
                                                    <div class="form-group clearfix">
                                                        <div class="icheck-danger d-inline">
                                                            <input type="radio" name="r2" disabled class="radSolicitud" checked id="radioTipoSolicitud1">
                                                            <label for="radioTipoSolicitud1" class="lblTipoSolicitud"></label>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="nombreOperador" class="col-form-label">Número solicitud:</label>
                                                </td>
                                                <td class="col-sm-9">
                                                    <div class="form-group row">
                                                        <div class="col-sm-4">
                                                            <input type="text" class="form-control form-control-sm" id="numeroSolicitud" name="numeroSolicitud" readonly>
                                                        </div>
                                                        <label form="fechaSolicitud" class="col-sm-3 col-form-label">Fecha solicitud:</label>
                                                        <div class="col-sm-5">
                                                            <input type="text" class="form-control form-control-sm" id="fechaSolicitud" readonly>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            @*1 INFORMACIÓN DEL EXPLOTADOR O SOLICITANTE*@
                                            <tr>
                                                <td colspan="2">
                                                    <div class="col-sm-12 text-sm align-top">
                                                        <small class="card-title text-muted"><u class="text-primary">INFORMACIÓN DEL EXPLOTADOR O SOLICITANTE</u></small>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="nombreOperador" class="col-form-label">Nombre del explotador:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="nombreOperador" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="numeroRucOperador" class="col-form-label">Número Ruc:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="numeroRucOperador" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="telefonoOperador" class="col-form-label">Télefono:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="telefonoOperador" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="direccionOperador" class="col-form-label">Télefono:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="direccionOperador" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="emailOperador" class="col-form-label">Correo o email:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="emailOperador" readonly></td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <div class="card h-100">
                                                        <table id="tablaAdjuntos" class="table table-bordered table-striped table-sm" style="width:100%">
                                                            <thead>
                                                                <tr class="text-center">
                                                                    <th>Código</th>
                                                                    <th>Documento</th>
                                                                    <th>Adjunto</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </td>
                                            </tr>
                                            @*1 INFORMACIÓN DEL LA AERONAVE*@
                                            <tr>
                                                <td colspan="2">
                                                    <div class="col-sm-12 text-sm align-top">
                                                        <small class="card-title text-muted"><u class="text-primary">INFORMACIÓN DE LA AERONAVE</u></small>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="fechaVigenciaSeguro" class="col-form-label">Vigencia seguro:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm col-sm-4 text-right" id="fechaVigenciaSeguro" readonly></td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <div class="card h-100">
                                                        <table id="tablaAeronaves" class="table table-bordered table-striped table-sm" style="width:100%">
                                                            <thead>
                                                                <tr class="text-center">
                                                                    <th>Mátricula</th>
                                                                    <th>Fabricante</th>
                                                                    <th>Modelo</th>
                                                                    <th>Mtow(kg)</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </td>
                                            </tr>
                                            @*3 INFORMACIÓN DEL VUELO*@
                                            <tr>
                                                <td colspan="2">
                                                    <div class="col-sm-12 text-sm align-top">
                                                        <small class="card-title text-muted"><u class="text-primary">INFORMACIÓN DEL VUELO</u></small>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="propositoVuelo" class="col-form-label">Proposito del vuelo:</label>
                                                </td>
                                                <td class="col-sm-9">
                                                    <input type="text" class="form-control form-control-sm" id="propositoVuelo" readonly>
                                                </td>
                                            </tr>
                                            <tr class="divVueloItinerario" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="vueloItinerario" class="col-form-label">Vuelo itinerario:</label>
                                                </td>
                                                <td class="col-sm-9">
                                                    <input type="text" class="form-control form-control-sm" id="vueloItinerario" readonly>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="especificarObjetoVuelo" class="col-form-label">Especificar objeto del vuelo y materia del transporte:</label>
                                                </td>
                                                <td class="col-sm-9">
                                                    <div class="form-group clearfix">
                                                        <div class="icheck-danger d-inline">
                                                            <input type="radio" name="r3" disabled class="radPropositoVuelo" checked id="radioPropositoVuelo">
                                                            <label for="radioPropositoVuelo" class="lblPropisitoVuelo"></label>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="numeroPasajeros" class="col-form-label">No. Pasajeros transportados:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm col-sm-5 text-right" id="numeroPasajeros" readonly></td>
                                            </tr>
                                            <tr>
                                                <td colspan="2">
                                                    <div class="card h-100">
                                                        <table id="tablaRutaVuelos" class="table table-bordered table-striped table-sm text-center" style="width:100%">
                                                            <thead>
                                                                <tr class="text-center">
                                                                    <th>Ruta Vuelo</th>
                                                                    <th>Derechos Solicitados</th>
                                                                    <th>Fecha Ida Exacta(Sin periodos)</th>
                                                                    <th>Fecha Vuelta</th>
                                                                    <th>Hora Estimada de Vuelo</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody></tbody>
                                                        </table>
                                                    </div>
                                                </td>
                                            </tr>
                                            @*3 INFORMACIÓN DEL FLETADOR*@
                                            <tr class="divFletador" style="display: none">
                                                <td colspan="2">
                                                    <div class="col-sm-12 text-sm align-top">
                                                        <small class="card-title text-muted"><u class="text-primary">INFORMACIÓN DEL FLETADOR</u></small>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr class="divFletador" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="nombreFletador" class="col-form-label">Nombre del Fletador:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="nombreFletador" readonly></td>
                                            </tr>
                                            <tr class="divFletador" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="telefonoFletador" class="col-form-label">Teléfono:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="telefonoFletador" readonly></td>
                                            </tr>
                                            <tr class="divFletador" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="direccionFletador" class="col-form-label">Dirección:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="direccionFletador" readonly></td>
                                            </tr>
                                            <tr class="divFletador" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="emailFletador" class="col-form-label">Correo o email:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="emailFletador" readonly></td>
                                            </tr>
                                            <tr class="divFletador" style="display: none">
                                                <td class="col-sm-3">
                                                    <label for="nombreRepresentanteFletador" class="col-form-label">Nombre del representante:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="nombreRepresentanteFletador" readonly></td>
                                            </tr>
                                            @*3 INFORMACIÓN DEL FACTURA*@
                                            <tr>
                                                <td colspan="2">
                                                    <div class="col-sm-12 text-sm align-top">
                                                        <small class="card-title text-muted"><u class="text-primary">INFORMACIÓN DEL FACTURADOR</u></small>
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="nombreCiaFactura" class="col-form-label">Compañia que factura:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="nombreCiaFactura" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="numeroRucFactura" class="col-form-label">CI/RUC:</label>
                                                </td>
                                                <td class="col-sm-9"> <input type="text" class="form-control form-control-sm" id="numeroRucFactura" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="telefonoFletador" class="col-form-label">Teléfono:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="telefonoFactura" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="direccionFletador" class="col-form-label">Dirección:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="direccionFactura" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="emailFletador" class="col-form-label">Email:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="emailFactura" readonly></td>
                                            </tr>
                                            <tr>
                                                <td class="col-sm-3">
                                                    <label for="observacionFletador" class="col-form-label">Observación:</label>
                                                </td>
                                                <td class="col-sm-9">  <input type="text" class="form-control form-control-sm" id="observacionFactura" readonly></td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
                <br />
                <a href="#" onclick="downloadSolicitud()"> Imprimir solicitud</a>
                <div class="form-group row">
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="col-sm-12 text-sm align-top">
                                <small class="card-title text-muted"><u class="text-primary">FORMA DE PAGO</u></small>
                            </div>
                            <div class="card-body">
                                <table style="width:100%; border-collapse: separate; border-spacing: 5px;">
                                    <tr>
                                        <td class="col-sm-4">
                                            <label for="numeroVuelos" class="col-form-label">No. vuelos:</label>
                                        </td>
                                        <td class="col-sm-8">
                                            <input type="text" class="form-control form-control-sm col-sm-4 text-right" id="numeroVuelos" readonly>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="col-sm-4">
                                            <label for="totalPagar" class="col-form-label">Total a pagar:</label>
                                        </td>
                                        <td class="col-sm-8">
                                            <input type="text" class="form-control form-control-sm col-sm-4 text-right" id="totalPagar" readonly>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="col-sm-4">
                                            <label for="formaPago" class="col-form-label">Forma pago:</label>
                                        </td>
                                        <td class="col-sm-8">
                                            <input type="text" class="form-control form-control-sm" id="formaPago" readonly>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="col-sm-4">
                                            <label for="codigoBanco" class="col-form-label">Banco:</label>
                                        </td>
                                        <td class="col-sm-8">
                                            @if (@ViewBag.ListaBancoPago != null)
                                            {
                                                @Html.DropDownList("select", ViewBag.ListaBancoPago as SelectList, new { @id = "ListaBancoPago", @disabled = "disabled", @class = "form-control form-control-sm" })
                                            }
                                        </td>
                                    </tr>
